{
	"expo": {
		"name": "idealism",
		"slug": "idealism",
		"version": "1.0.0",
		"orientation": "portrait",
		"scheme": "idealism",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro",
			"output": "server"
		},
		"plugins": [
			[
				"expo-notifications",
				{
					"icon": "./assets/icon.png",
					"color": "#000000"
				}
			]
		]
	}
}
